<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Obl3</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
    <session-config>
    <session-timeout>15</session-timeout>
  	</session-config>
    <servlet>
        <servlet-name>handleliste</servlet-name>
        <servlet-class>handleliste.handleliste</servlet-class>
        <init-param>
            <param-name>hardcodedPassword</param-name>
            <param-value>123test</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>handleliste</servlet-name>
        <url-pattern>/handleliste</url-pattern>
    </servlet-mapping>
</web-app>